<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="442.0" minWidth="600.0" prefHeight="442.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs1302.pantry_tracker.view.MainWindow">
   <children>
      <Label layoutX="96.0" layoutY="48.0" prefHeight="17.0" prefWidth="120.0" text="Name For New Food" textAlignment="CENTER" wrapText="true" />
      <TextField fx:id="foodName" layoutX="81.0" layoutY="87.0" promptText="enter name" />
      <Label layoutX="272.0" layoutY="48.0" prefHeight="17.0" prefWidth="99.0" text="Type Of New Food" />
      <ComboBox fx:id="typeOfFood" layoutX="247.0" layoutY="87.0" prefWidth="150.0" />
      <Button layoutX="81.0" layoutY="386.0" mnemonicParsing="false" onAction="#addFoodToPantry" text="Submit" />
      <ListView fx:id="selectFood" layoutX="156.0" layoutY="211.0" prefHeight="200.0" prefWidth="200.0" />
      <Label fx:id="settingTheQuantity" layoutX="452.0" layoutY="48.0" prefHeight="17.0" prefWidth="120.0" text="setting the quantity" textAlignment="CENTER" wrapText="true" />
      <TextField fx:id="quantity" layoutX="423.0" layoutY="87.0" promptText=" enter quantity" />
      <Button fx:id="increment" layoutX="632.0" layoutY="53.0" mnemonicParsing="false" text="+" />
      <Button fx:id="decrement" layoutX="634.0" layoutY="100.0" mnemonicParsing="false" text="-" />
   </children>
</AnchorPane>
